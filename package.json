    "type": "git",
    "url": "git+ssh://git@bitbucket.org/ibarsi/drizzle.git"
  },
  "author": "Igor Barsi <ibarsi92@gmail.com> (https://github.com/ibarsi)",
  "license": "MIT",
  "homepage": "https://bitbucket.org/ibarsi/drizzle#readme",
  "dependencies": {
    "cheerio": "0.22.0",
    "isomorphic-fetch": "2.2.1",
    "twit": "2.2.5",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-preset-latest-minimal": "1.1.2"
  }
}
